<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SearchCustomerActivity2">

    <TextView
        android:id="@+id/textView17"
        android:layout_width="359dp"
        android:layout_height="59dp"
        android:layout_marginStart="26dp"
        android:layout_marginTop="10dp"
        android:text="Search Customer"
        android:textAlignment="center"
        android:textColor="#00EEEE"
        android:textSize="40dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="104dp"
        android:layout_height="85dp"
        android:layout_marginStart="22dp"
        android:layout_marginTop="19dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/profileblue" />

    <TextView
        android:id="@+id/txtUsername"
        android:layout_width="228dp"
        android:layout_height="46dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="36dp"
        android:text=" "
        android:textSize="50dp"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="37dp"
        android:layout_marginTop="35dp"
        android:text="Index Number"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView17" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginTop="24dp"
        android:text="NIC"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginTop="28dp"
        android:text="Birthdate"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginTop="24dp"
        android:text="Gender"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView13" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginTop="16dp"
        android:text="Phone Number"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView14" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginTop="20dp"
        android:text="Address"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView15" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginTop="28dp"
        android:text="Name"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10" />

    <TextView
        android:id="@+id/txtIndexNo"
        android:layout_width="202dp"
        android:layout_height="25dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="41dp"
        android:text=" "
        app:layout_constraintStart_toEndOf="@+id/textView10"
        app:layout_constraintTop_toBottomOf="@+id/textView17" />

    <TextView
        android:id="@+id/txtName"
        android:layout_width="202dp"
        android:layout_height="25dp"
        android:layout_marginStart="102dp"
        android:layout_marginTop="24dp"
        android:text=" "
        app:layout_constraintStart_toEndOf="@+id/textView11"
        app:layout_constraintTop_toBottomOf="@+id/txtIndexNo" />

    <TextView
        android:id="@+id/txtNIC"
        android:layout_width="202dp"
        android:layout_height="25dp"
        android:layout_marginStart="123dp"
        android:layout_marginTop="28dp"
        android:text=" "
        app:layout_constraintStart_toEndOf="@+id/textView12"
        app:layout_constraintTop_toBottomOf="@+id/txtName" />

    <TextView
        android:id="@+id/txtBirthdate"
        android:layout_width="202dp"
        android:layout_height="25dp"
        android:layout_marginStart="74dp"
        android:layout_marginTop="28dp"
        android:text=" "
        app:layout_constraintStart_toEndOf="@+id/textView13"
        app:layout_constraintTop_toBottomOf="@+id/txtNIC" />

    <TextView
        android:id="@+id/txtGender"
        android:layout_width="202dp"
        android:layout_height="25dp"
        android:layout_marginStart="92dp"
        android:layout_marginTop="26dp"
        android:text=" "
        app:layout_constraintStart_toEndOf="@+id/textView14"
        app:layout_constraintTop_toBottomOf="@+id/txtBirthdate" />

    <TextView
        android:id="@+id/txtPnNo"
        android:layout_width="202dp"
        android:layout_height="25dp"
        android:layout_marginStart="23dp"
        android:layout_marginTop="20dp"
        android:text=" "
        app:layout_constraintStart_toEndOf="@+id/textView15"
        app:layout_constraintTop_toBottomOf="@+id/txtGender" />

    <TextView
        android:id="@+id/txtAddress"
        android:layout_width="202dp"
        android:layout_height="25dp"
        android:layout_marginStart="81dp"
        android:layout_marginTop="20dp"
        android:text=" "
        app:layout_constraintStart_toEndOf="@+id/textView16"
        app:layout_constraintTop_toBottomOf="@+id/txtPnNo" />

</androidx.constraintlayout.widget.ConstraintLayout>